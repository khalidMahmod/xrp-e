    return this.populate = function(event, data) {
      var i, item, len, ref;
      if (!_.isEmpty(data.orders)) {
        ref = data.orders;
        for (i = 0, len = ref.length; i < len; i++) {
          item = ref[i];
          this.addOrUpdateItem(item);
        }
      }
      return this.checkEmpty();
    };
  };

