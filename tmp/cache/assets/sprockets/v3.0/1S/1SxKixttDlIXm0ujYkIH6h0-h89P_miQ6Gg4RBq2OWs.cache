I"ª(function() {
  window.sfx_warning = function() {
    return window.sfx('warning');
  };

  window.sfx_success = function() {
    return window.sfx('success');
  };

  window.sfx = function(kind) {
    var s;
    s = $("#" + kind + "-fx")[0];
    if (Cookies.get('sound') === 'false') {
      return;
    }
    if (!s.play) {
      return;
    }
    s.pause();
    s.currentTime = 0;
    return s.play();
  };

}).call(this);
