<!DOCTYPE html>
<html lang="en">
  <head>
    <%= render 'shared/meta' %>
    <%= render 'shared/sentry' %>
    <%= csrf_meta_tags %>
    <%= include_gon %>
    <%= stylesheet_link_tag 'api_v2', media: 'all' %>
    <%= render 'shared/html5' %>
    <%= javascript_include_tag 'api_v2' %>



  </head>

  <body data-lang="<%= locale_name %>" id="<%= body_id %>">
    <%= render 'shared/alert' %>
    <div id="wrap">
      <%= content_for?(:header) ? yield(:header) : render('shared/header') %>
    </div>
    <div class="container">
      <%= yield %>
    </div>
    <%= render 'shared/footer' %>
  </body>
</html>
